{
  "expo": {
    "name": "bolt-expo-nativewind",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d51d3db5-bc3f-47a3-a5a1-c20433ac84ec"
      }
    },
    "android": {
      "package": "com.dhavansinghal.boltexponativewind"
    },
    "build": {
      "android": {
        "gradleCommand": ":app:bundleRelease",
        "env": {
          "EXPO_ANDROID_KOTLIN_VERSION": "1.9.25"
        }
      },
      "preview": {
        "android": {
          "buildType": "apk"
        }
      },
    "production": {
      "android": {
        "buildType": "aab"
      }
    }

    }
  }
}

